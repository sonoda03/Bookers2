<!-- ユーザー一覧 -->
<h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
    </thead>
    
    <tbody>
      <% @users.each do |users| %>
        <tr>
          <td><%= image_tag users.get_profile_image(100,100) %></td>
          <td><%= users.name %></td>
          <td><%= link_to "Show", user_path(users.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>